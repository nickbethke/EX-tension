{
    "name": "EX|tension",
    "version": "1.1.3",
    "description": "A helper, all devs need.",
    "permissions": [
        "activeTab",
        "declarativeContent",
        "tabs",
        "storage",
        "https://*/*",
        "http://*/*"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    },
    "options_page": "content/options.html",
    "browser_action": {
        "default_popup": "content/popup.html",
        "default_icon": {
            "128": "images/logo_size.png"
        }
    },
    "icons": {
        "16": "images/logo_size_16.png",
        "32": "images/logo_size_32.png",
        "48": "images/logo_size_48.png",
        "128": "images/logo_size.png"
    },
    "manifest_version": 2
}